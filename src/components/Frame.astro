---
const { width } = Astro.props;
const { height } = Astro.props;
---

<div class="frame_container">

</div>

<style define:vars={{width, height}}>
    .frame_container
    {
        position: fixed;
        display: flex;
        align-items: center;
        justify-content: center;
        width: var(--width);
        height: var(--height);
        background: #ecf0f3;
        border: none;
        border-radius: 32px;
        box-shadow: inset 8px 8px 16px rgb(186, 196, 214);
        z-index: -1;
    }
</style>